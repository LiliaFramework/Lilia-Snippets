{
    "Lilia Module": {
        "prefix": "lilia.module",
        "body": [
            "local MODULE = MODULE",
            "",
            "MODULE.name = \"NAME\"",
            "MODULE.desc = \"DESCRIPTION\"",
            "MODULE.author = \"AUTHOR\"",
            "MODULE.schema = \"SCHEMA\""
        ],
        "description": "Create a new module"
    },

    "Lilia Item": {
        "prefix": "lilia.item",
        "body": [
          "ITEM.name = \"NAME\"",
          "ITEM.desc = \"DESCRIPTION\"",
          "ITEM.model = \"MODEL\"",
          "ITEM.width = 1",
          "ITEM.height = 1",
          "ITEM.price = 1",
          "ITEM.category = \"CategoryName\"",
          "",
          "ITEM.iconCam = {\n    pos = Vector(0, 0, 0), \n    ang = Angle(0, 0, 0),\n    fov = 70\n}",
          "",
          "ITEM.functions.Option = {",
          "    name = \"OptionName\",",
          "    onClick = function(item)",
          "        -- onClick functionality here",
          "    end,",
          "    onRun = function(item)",
          "        -- onRun functionality here",
          "        return false",
          "    end,",
          "    onCanRun = function(item)",
          "        -- onCanRun checks here",
          "    end,",
          "}",
          "",
          "function ITEM:getName()",
          "    return self.name",
          "end",
          "",
          "function ITEM:getDesc()",
          "    return self.desc",
          "end"
        ],
        "description": "Create a new item with default values for a Lilia gamemode"
    },

    "Lilia Outfit Item": {
        "prefix": "lilia.item.outfit",
        "body": [
            "ITEM.name = \"NAME\"",
            "ITEM.desc = \"DESCRIPTION\"",
            "ITEM.model = \"MODEL\"",
            "ITEM.width = 1",
            "ITEM.height = 1",
            "ITEM.outfitCategory = \"CATEGORY\"",
            "ITEM.newSkin = 1",
            "ITEM.bodyGroups = {",
            "    [\"BODYGROUPNAME\"] = 1,",
            "}",
            "ITEM.replacements = \"MODEL\""
        ],
        "description": "Create a new outfit item with default values for a Lilia gamemode"
    },

